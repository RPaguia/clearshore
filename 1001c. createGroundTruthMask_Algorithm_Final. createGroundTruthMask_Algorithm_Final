   ],
   "source": [
    "import cv2\n",
    "import numpy as np\n",
    "import glob\n",
    "import os\n",
    "import pandas as pd\n",
    "from datetime import datetime\n",
    "\n",
    "\n",
    "# Function to load image files from subfolders within a directory\n",
    "def load_images_from_folder(folder):\n",
    "    images = []\n",
    "    filenames = []\n",
    "    for root, dirs, files in os.walk(folder):\n",
    "        for file_type in ('*.jpg', '*.jpeg', '*.png', '*.bmp'):\n",
    "            for filename in glob.glob(os.path.join(root, file_type)):\n",
    "                img = cv2.imread(filename)\n",
    "                if img is not None:\n",
    "                    images.append(img)\n",
    "                    filenames.append(filename)\n",
    "                else:\n",
    "                    print(f\"Failed to load {filename}\")\n",
    "    return images, filenames\n",
    "\n",
    "# Function to create the Ground Truth Mask\n",
    "def create_ground_truth_mask(target_img, reference_images, target_filename):\n",
    "    # Convert target image to grayscale\n",
    "    target_gray = cv2.cvtColor(target_img, cv2.COLOR_BGR2GRAY)\n",
    "    ground_truth_mask = np.zeros(target_gray.shape, dtype=np.uint8)\n",
    "    \n",
    "    # Initialize the maximum reference image\n",
    "    max_reference_gray = np.zeros(target_gray.shape, dtype=np.uint8)\n",
    "    \n",
    "    # Centermost pixel coordinates\n",
    "    center_x = target_gray.shape[1] // 2\n",
    "    center_y = target_gray.shape[0] // 2\n",
    "\n",
    "    # Determine the highest observed pixel values from reference images\n",
    "    for reference_img, reference_filename in reference_images:\n",
    "        resized_ref_img = cv2.resize(reference_img, (target_gray.shape[1], target_gray.shape[0]))\n",
    "        reference_gray = cv2.cvtColor(resized_ref_img, cv2.COLOR_BGR2GRAY)\n",
    "        max_reference_gray = np.maximum(max_reference_gray, reference_gray)\n",
    "        \n",
    "        # Print debug statements for the current reference image\n",
    "        print(f\"Target image: {target_filename}\")\n",
    "        print(f\"Centermost pixel value in target image: {target_gray[center_y, center_x]}\")\n",
    "        print(f\"Reference image: {reference_filename}\")\n",
    "        print(f\"Centermost pixel value in reference image: {reference_gray[center_y, center_x]}\")\n",
    "\n",
    "    # Print debug statements for the max_reference_gray\n",
    "    print(f\"Centermost pixel value in max_reference_gray: {max_reference_gray[center_y, center_x]}\")\n",
    "\n",
    "    # Update ground truth mask based on the comparison\n",
    "    ground_truth_mask = np.where(target_gray > 1.00 * max_reference_gray, 255, 0).astype(np.uint8)\n",
    "\n",
    "    # Print the result for the centermost pixel in the ground truth mask\n",
    "    mask_value = \"cloud\" if ground_truth_mask[center_y, center_x] == 255 else \"non-cloud\"\n",
    "    print(f\"Result for centermost pixel in ground truth mask: {mask_value}\")\n",
    "\n",
    "    return ground_truth_mask\n",
    "\n",
    "# Function for calculating accuracy between two masks and additional metrics\n",
    "def calculate_metrics(detected_mask, reference_mask):\n",
    "    TP = np.sum((detected_mask == 255) & (reference_mask == 255))\n",
    "    TN = np.sum((detected_mask == 0) & (reference_mask == 0))\n",
    "    FP = np.sum((detected_mask == 255) & (reference_mask == 0))\n",
    "    FN = np.sum((detected_mask == 0) & (reference_mask == 255))\n",
    "\n",
    "    accuracy = (TP + TN) / (TP + TN + FP + FN) * 100 if (TP + TN + FP + FN) > 0 else 0\n",
    "    precision = TP / (TP + FP) if (TP + FP) > 0 else 0\n",
    "    recall = TP / (TP + FN) if (TP + FN) > 0 else 0\n",
    "    f1_score = 2 * (precision * recall) / (precision + recall) if (precision + recall) > 0 else 0\n",
    "\n",
    "    return accuracy, precision, recall, f1_score, TP, TN, FP, FN\n",
    "\n",
    "# Function to draw bounding boxes around cloud regions\n",
    "def draw_bounding_boxes(image, mask):\n",
    "    height, width, _ = image.shape\n",
    "    num_labels, labels, stats, centroids = cv2.connectedComponentsWithStats(mask, connectivity=8)\n",
    "    max_area = 0\n",
    "    best_bbox = None\n",
    "\n",
    "    for i in range(1, num_labels):  # Skip the background label (0)\n",
    "        x, y, w, h, area = stats[i]\n",
    "        if x + w > width:\n",
    "            w = width - x\n",
    "        if y + h > height:\n",
    "            h = height - y\n",
    "        component_mask = (labels == i).astype(np.uint8)\n",
    "        cloud_pixel_percentage = (np.sum(component_mask) / area) * 100\n",
    "        if cloud_pixel_percentage >= 50 and area > max_area:\n",
    "            max_area = area\n",
    "            best_bbox = (x, y, w, h)\n",
    "\n",
    "    if best_bbox is not None:\n",
    "        x, y, w, h = best_bbox\n",
    "        cv2.rectangle(image, (x, y), (x + w, y + h), (0, 255, 0), 2)\n",
    "\n",
    "    return image\n",
    "\n",
    "# Function to generate a unique filename for the report\n",
    "def generate_report_filename(output_folder, hueLow, hueHigh, satLow, satHigh, valLow, valHigh, dataset_folder, extension):\n",
    "    counter = 1\n",
    "    base_filename = f\"({hueLow},{hueHigh})_({satLow},{satHigh})_({valLow},{valHigh})_{os.path.basename(dataset_folder)}_metrics\"\n",
    "    while os.path.exists(os.path.join(output_folder, f\"{base_filename}_{counter}.{extension}\")):\n",
    "        counter += 1\n",
    "    return os.path.join(output_folder, f\"{base_filename}_{counter}.{extension}\")\n",
    "\n",
    "# Function to save mask with specific filename format\n",
    "def save_mask(mask, mask_type, hueLow, hueHigh, satLow, satHigh, valLow, valHigh, dataset_folder, original_filename):\n",
    "    mask_folder = r\"D:\\cdm-mlb-cls\\100_groundTruth\\images\\season\\groundTruthMasks_season\\TU_S2_fall\"\n",
    "    if not os.path.exists(mask_folder):\n",
    "        os.makedirs(mask_folder)\n",
    "    mask_filename = f\"{mask_type}Mask_({hueLow},{hueHigh})({satLow},{satHigh})({valLow},{valHigh})_{os.path.basename(dataset_folder)}_{os.path.basename(original_filename)}\"\n",
    "    cv2.imwrite(os.path.join(mask_folder, mask_filename), mask)\n",
    "\n",
    "# Load target images from the specified folder\n",
    "target_folder = r\"D:\\cdm-mlb-cls\\100_groundTruth\\images\\season\\TU_S2_fall\"\n",
    "target_images, target_filenames = load_images_from_folder(target_folder)\n",
    "\n",
    "# Load reference images from the specified folder\n",
    "reference_folder = r\"D:\\cdm-mlb-cls\\100_groundTruth\\cloudFreeImages\\Lake_Tuggeranong\\TU_S2_fall\"\n",
    "reference_images, reference_filenames = load_images_from_folder(reference_folder)\n",
    "\n",
    "# Check if images are loaded\n",
    "if len(target_images) == 0 or len(reference_images) == 0:\n",
    "    print(\"No images found in the specified folder.\")\n",
    "    exit()\n",
    "\n",
    "# Define the output folder\n",
    "output_folder = r\"D:\\cdm-mlb-cls\\100_groundTruth\\images\\season\\groundTruthMasks_season\\TTU_S2_fall\\output\"\n",
    "if not os.path.exists(output_folder):\n",
    "    os.makedirs(output_folder)\n",
    "\n",
    "# Initialize list to store metrics for the Excel report\n",
    "metrics_list_threshold = []\n",
    "metrics_list_filtered = []\n",
    "\n",
    "# Loop through all target images to process and save masks\n",
    "for idx, target_img in enumerate(target_images):\n",
    "    start_time = datetime.now()\n",
    "    filename = target_filenames[idx]\n",
    "\n",
    "    # Create Ground Truth Mask\n",
    "    ground_truth_mask = create_ground_truth_mask(target_img, zip(reference_images, reference_filenames), filename)\n",
    "    save_mask(ground_truth_mask, \"Ground_Truth\", 0, 0, 0, 0, 0, 0, target_folder, filename)\n",
    "\n",
    "    \n",
    "\n",
    "\n",
    "    # Convert image to HSV color space and create masks\n",
    "    frameHSV = cv2.cvtColor(target_img, cv2.COLOR_BGR2HSV)\n",
    "    hueLow, hueHigh = 0, 179\n",
    "    satLow, satHigh = 0, 255\n",
    "    valLow, valHigh = 0, 255\n",
    "    myMask = cv2.inRange(frameHSV, np.array([hueLow, satLow, valLow]), np.array([hueHigh, satHigh, valHigh]))\n",
    "\n",
    "    # Calculate filtered mask where a pixel is PCP if 6 out of its 8 surrounding pixels are PCP\n",
    "    kernel = np.ones((3, 3), np.uint8)\n",
    "    neighbor_count = cv2.filter2D(myMask // 255, -1, kernel) - (myMask // 255)\n",
    "    filtered_mask = np.where(neighbor_count >= 6, 255, 0).astype(np.uint8)\n",
    "\n",
    "    # Calculate metrics between the threshold mask and the ground truth mask\n",
    "    accuracy_threshold, precision_threshold, recall_threshold, f1_score_threshold, TP_threshold, TN_threshold, FP_threshold, FN_threshold = calculate_metrics(myMask, ground_truth_mask)\n",
    "    \n",
    "    # Calculate metrics between the filtered mask and the ground truth mask\n",
    "    accuracy_filtered, precision_filtered, recall_filtered, f1_score_filtered, TP_filtered, TN_filtered, FP_filtered, FN_filtered = calculate_metrics(filtered_mask, ground_truth_mask)\n",
    "\n",
    "    # Print debug statements for threshold mask metrics\n",
    "    total_pixels_threshold_mask = np.size(myMask)\n",
    "    cloud_pixels_threshold_mask = np.sum(myMask == 255)\n",
    "    non_cloud_pixels_threshold_mask = total_pixels_threshold_mask - cloud_pixels_threshold_mask\n",
    "    pixels_changed_cloud_to_noncloud_threshold = np.sum((myMask == 255) & (filtered_mask == 0))\n",
    "\n",
    "    print(f\" \")\n",
    "    print(f\"Threshold Mask Metrics for {filename}:\")\n",
    "    print(f\"TP: {TP_threshold}, TN: {TN_threshold}, FP: {FP_threshold}, FN: {FN_threshold}\")\n",
    "    print(f\"Total Pixels in Threshold Mask: {total_pixels_threshold_mask}\")\n",
    "    print(f\"Count of Cloud Pixels in Threshold Mask: {cloud_pixels_threshold_mask}\")\n",
    "    print(f\"Non-cloud pixels in Threshold Mask: {non_cloud_pixels_threshold_mask}\")\n",
    "    print(f\"Count of Pixels changed (cloud to non-cloud) from threshold mask to filtered mask: {pixels_changed_cloud_to_noncloud_threshold}\")\n",
    "\n",
    "    # Print debug statements for filtered mask metrics\n",
    "    total_pixels_filtered_mask = np.size(filtered_mask)\n",
    "    cloud_pixels_filtered_mask = np.sum(filtered_mask == 255)\n",
    "    non_cloud_pixels_filtered_mask = total_pixels_filtered_mask - cloud_pixels_filtered_mask\n",
    "    pixels_changed_cloud_to_noncloud_filtered = np.sum((filtered_mask == 255) & (myMask == 0))\n",
    "\n",
    "    print(f\" \")\n",
    "    print(f\"Filtered Mask Metrics for {filename}:\")\n",
    "    print(f\"TP: {TP_filtered}, TN: {TN_filtered}, FP: {FP_filtered}, FN: {FN_filtered}\")\n",
    "    print(f\"Total Pixels in Filtered Mask: {total_pixels_filtered_mask}\")\n",
    "    print(f\"Count of Cloud Pixels in Filtered Mask: {cloud_pixels_filtered_mask}\")\n",
    "    print(f\"Non-cloud pixels in Filtered Mask: {non_cloud_pixels_filtered_mask}\")\n",
    "    print(f\"Count of Pixels changed (cloud to non-cloud) from threshold mask to filtered mask: {pixels_changed_cloud_to_noncloud_filtered}\")\n",
    "\n",
    "    # Save the individual masks in original size\n",
    "    save_mask(myMask, \"Threshold\", hueLow, hueHigh, satLow, satHigh, valLow, valHigh, target_folder, filename)\n",
    "    save_mask(filtered_mask, \"Filtered\", hueLow, hueHigh, satLow, satHigh, valLow, valHigh, target_folder, filename)\n",
    "\n",
    "    # Add additional details like Sky Condition and Season (assuming these are available in some metadata)\n",
    "    sky_condition = \"Unknown\"  # Replace with actual value\n",
    "    season = \"Unknown\"  # Replace with actual value\n",
    "\n",
    "    # Record the metrics for threshold mask\n",
    "    processing_time_threshold = (datetime.now() - start_time).total_seconds()\n",
    "    metrics_list_threshold.append({\n",
    "        \"Filename\": filename,\n",
    "        \"Sky Condition\": sky_condition,\n",
    "        \"Season\": season,\n",
    "        \"Total Pixels in Threshold Mask\": total_pixels_threshold_mask,\n",
    "        \"Cloud Pixels in Threshold Mask\": cloud_pixels_threshold_mask,\n",
    "        \"Non-cloud Pixels in Threshold Mask\": non_cloud_pixels_threshold_mask,\n",
    "        \"Pixels changed (cloud to non-cloud)\": pixels_changed_cloud_to_noncloud_threshold,\n",
    "        \"TP in Threshold Mask\": TP_threshold,\n",
    "        \"FP in Threshold Mask\": FP_threshold,\n",
    "        \"TN in Threshold Mask\": TN_threshold,\n",
    "        \"FN in Threshold Mask\": FN_threshold,\n",
    "        \"Accuracy in Threshold Mask\": accuracy_threshold,\n",
    "        \"Precision in Threshold Mask\": precision_threshold,\n",
    "        \"Recall in Threshold Mask\": recall_threshold,\n",
    "        \"F1-Score in Threshold Mask\": f1_score_threshold,\n",
    "        \"Processing Time in Threshold Mask\": processing_time_threshold,\n",
    "        \"Hue Low in Threshold Mask\": hueLow,\n",
    "        \"Hue High in Threshold Mask\": hueHigh,\n",
    "        \"Sat Low in Threshold Mask\": satLow,\n",
    "        \"Sat High in Threshold Mask\": satHigh,\n",
    "        \"Val Low in Threshold Mask\": valLow,\n",
    "        \"Val High in Threshold Mask\": valHigh\n",
    "    })\n",
    "\n",
    "    # Record the metrics for filtered mask\n",
    "    processing_time_filtered = (datetime.now() - start_time).total_seconds()\n",
    "    metrics_list_filtered.append({\n",
    "        \"Filename\": filename,\n",
    "        \"Sky Condition\": sky_condition,\n",
    "        \"Season\": season,\n",
    "        \"Total Pixels in Filtered Mask\": total_pixels_filtered_mask,\n",
    "        \"Cloud Pixels in Filtered Mask\": cloud_pixels_filtered_mask,\n",
    "        \"Non-cloud Pixels in Filtered Mask\": non_cloud_pixels_filtered_mask,\n",
    "        \"Pixels changed (cloud to non-cloud)\": pixels_changed_cloud_to_noncloud_filtered,\n",
    "        \"TP in Filtered Mask\": TP_filtered,\n",
    "        \"FP in Filtered Mask\": FP_filtered,\n",
    "        \"TN in Filtered Mask\": TN_filtered,\n",
    "        \"FN in Filtered Mask\": FN_filtered,\n",
    "        \"Accuracy in Filtered Mask\": accuracy_filtered,\n",
    "        \"Precision in Filtered Mask\": precision_filtered,\n",
    "        \"Recall in Filtered Mask\": recall_filtered,\n",
    "        \"F1-Score in Filtered Mask\": f1_score_filtered,\n",
    "        \"Processing Time in Filtered Mask\": processing_time_filtered,\n",
    "        \"Hue Low in Filtered Mask\": hueLow,\n",
    "        \"Hue High in Filtered Mask\": hueHigh,\n",
    "        \"Sat Low in Filtered Mask\": satLow,\n",
    "        \"Sat High in Filtered Mask\": satHigh,\n",
    "        \"Val Low in Filtered Mask\": valLow,\n",
    "        \"Val High in Filtered Mask\": valHigh\n",
    "    })\n",
    "\n",
    "# Save the metrics to an Excel file with a unique name\n",
    "excel_filename = generate_report_filename(output_folder, hueLow, hueHigh, satLow, satHigh, valLow, valHigh, target_folder, 'xlsx')\n",
    "with pd.ExcelWriter(excel_filename) as writer:\n",
    "    pd.DataFrame(metrics_list_threshold).to_excel(writer, sheet_name='Threshold Mask Metrics', index=False)\n",
    "    pd.DataFrame(metrics_list_filtered).to_excel(writer, sheet_name='Filtered Mask Metrics', index=False)\n",
    "\n",
    "# Save the metrics to a CSV file with a unique name\n",
    "csv_filename = generate_report_filename(output_folder, hueLow, hueHigh, satLow, satHigh, valLow, valHigh, target_folder, 'csv')\n",
    "pd.DataFrame(metrics_list_threshold).to_csv(f\"{csv_filename}_threshold.csv\", index=False)\n",
    "pd.DataFrame(metrics_list_filtered).to_csv(f\"{csv_filename}_filtered.csv\", index=False)\n",
    "\n",
    "print(f\"Reports saved to: {excel_filename} and {csv_filename}_threshold.csv, {csv_filename}_filtered.csv\")\n",
    "\n",
    "\n"
   ]
